#include<vector>
using namespace std;
int main()
 {
    int T;
    cin>>T;
	for(int i =0;i < T; i++)
	{
	    int n;
	    cin>>n;
	    vector<int> v;
	    for(int i = 0; i < n; i++)
	    {
	        int ip;
	        cin>>ip;
	        v.push_back(ip);
	    }
	    for(int i =n-1; i>=0; i--)
	    {
	        cout<<v[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
